{
    "name": "pizza-di-napoli",
    "authors": [
        {
            "name": "marianne-piquet-nowak",
            "email": "mariannexpiquet@gmail.com"
        }
    ],
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        }
    ],
    "require": {
        "johnpbloch/wordpress": "^5.1",
        "wpackagist-theme/twentynineteen": "*",
        "wpackagist-plugin/hello-dolly": "^1.7",
        "wpackagist-plugin/contact-form-7": "^5.1",
        "wpackagist-plugin/advanced-custom-fields-font-awesome": "^3.1",
        "wpackagist-plugin/simply-show-hooks": "^1.2",
        "wpackagist-plugin/what-the-file": "^1.5"
    },
    "extra": {
        "wordpress-install-dir": "wp",
        "installer-paths": {
            "content/plugins/{$name}/": ["type:wordpress-plugin"],
            "content/themes/{$name}/": ["type:wordpress-theme"]
        }
    }
}
